srcdir      = @srcdir@
top_srcdir  = @top_srcdir@
include $(top_srcdir)/Rules

INC =	$(xcininc)

SRC =	enclist.c charset.c utf8.c big5.c big5hkscs.c gb2312.c gbk.c
OBJ =	$(SRC:.c=.lo)

.PHONY: depend clean distclean

all: $(OBJ)

include .depend

depend:
	$(Dep_Rule) | $(makedep) $(rules) LC_Rule lo > .depend

clean:
	rm -f $(OBJ) *.o core
distclean: clean
	rm -rf Makefile .libs
