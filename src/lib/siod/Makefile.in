srcdir      = @srcdir@
top_srcdir  = @top_srcdir@
include $(top_srcdir)/Rules

DEFS = -Dunix
INC = $(xcininc)
SRC = slib.c sliba.c 
OBJ = $(SRC:.c=.lo)

.PHONY: depend clean distclean

all: $(OBJ)

include .depend

depend:
	$(Dep_Rule) | $(makedep) $(rules) LC_Rule lo > .depend

clean:
	rm -f $(OBJ) *.o core
distclean: clean
	rm -rf Makefile .libs
