CC          = @CC@
CFLAGS      = @CFLAGS@
CPPFLAGS    = @CPPFLAGS@
DEFS        = @DEFS@ -Dunix
RANLIB      = @RANLIB@
LN_S        = @LN_S@
@SET_MAKE@

srcdir      = @srcdir@
top_srcdir  = @top_srcdir@
prefix      = @prefix@
exec_prefix = @exec_prefix@
LIBTOOL     = $(top_srcdir)/libtool

xcininc	    = -I$(top_srcdir)/src/include
SRC         = slib.c sliba.c 
OBJ         = $(SRC:.c=.lo)
INC         = $(xcininc)

all: $(OBJ)

slib.lo: slib.c siod.h siodp.h ../../include/config.h
	$(LIBTOOL) --mode=compile $(CC) $(CFLAGS) $(INC) $(DEFS) -c -o $@ slib.c

sliba.lo: sliba.c siod.h siodp.h
	$(LIBTOOL) --mode=compile $(CC) $(CFLAGS) $(INC) $(DEFS) -c -o $@ sliba.c


.PHONY: clean distclean
clean:
	rm -f $(OBJ) *.o core
distclean: clean
	rm -rf Makefile .depend .libs
